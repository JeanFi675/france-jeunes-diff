{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "strict": true,
    "skipLibCheck": true,
    "jsxImportSource": "astro",
    "jsx": "react-jsx",
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "allowJs": true
  }
}
